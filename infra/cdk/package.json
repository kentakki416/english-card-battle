{
  "name": "english-card-battle-cdk",
  "version": "0.1.0",
  "bin": {
    "english-card-battle-cdk": "bin/english-card-battle-cdk.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "bootstrap": "cdk bootstrap aws://300126371565/ap-northeast-1",
    "deploy:dev": "ENVIRONMENT=dev cdk deploy --profile dev",
    "deploy:dev:no-execute": "ENVIRONMENT=dev cdk deploy --profile dev --no-execute",
    "deploy:stg": "ENVIRONMENT=stg cdk deploy --profile dev",
    "deploy:stg:no-execute": "ENVIRONMENT=stg cdk deploy --profile dev --no-execute",
    "deploy:prd": "ENVIRONMENT=prd cdk deploy --profile dev",
    "deploy:prd:no-execute": "ENVIRONMENT=prd cdk deploy --profile dev --no-execute",
    "destroy:dev": "ENVIRONMENT=dev cdk destroy --profile dev",
    "destroy:stg": "ENVIRONMENT=stg cdk destroy --profile dev",
    "destroy:prd": "ENVIRONMENT=prd cdk destroy --profile dev",
    "diff:dev": "ENVIRONMENT=dev cdk diff --profile dev --no-fail",
    "diff:stg": "ENVIRONMENT=stg cdk diff --profile dev --no-fail",
    "diff:prd": "ENVIRONMENT=prd cdk diff --profile dev --no-fail",
    "list:dev": "ENVIRONMENT=dev cdk list --profile dev",
    "list:stg": "ENVIRONMENT=stg cdk list --profile dev",
    "list:prd": "ENVIRONMENT=prd cdk list --profile dev",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "nag-check:dev": "ENVIRONMENT=dev NAG_CHECK=true cdk synth",
    "nag-check:stg": "ENVIRONMENT=stg NAG_CHECK=true cdk synth",
    "nag-check:prd": "ENVIRONMENT=prd NAG_CHECK=true cdk synth"
  },
  "devDependencies": {
    "@aws-sdk/client-dynamodb": "^3.848.0",
    "@eslint/js": "^9.31.0",
    "@types/jest": "^29.5.14",
    "@types/node": "20.7.1",
    "aws-cdk": "^2.1021.0",
    "cdk-nag": "^2.36.43",
    "esbuild": "^0.25.8",
    "eslint": "^9.31.0",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "globals": "^16.3.0",
    "jest": "^29.7.0",
    "jest-serializer-path": "^0.1.0",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.1",
    "typescript": "~5.2.2",
    "typescript-eslint": "^8.37.0"
  },
  "dependencies": {
    "aws-cdk-lib": "2.189.1",
    "constructs": "^10.0.0",
    "source-map-support": "^0.5.21"
  }
}
