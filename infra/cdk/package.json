{
  "name": "english-card-battle-cdk",
  "version": "0.1.0",
  "bin": {
    "english-card-battle-cdk": "bin/english-card-battle-cdk.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "bootstrap": "cdk bootstrap aws://300126371565/ap-northeast-1",
    "deploy:dev": "cdk deploy --profile dev",
    "deploy:dev:no-execute": "cdk deploy --profile dev --no-execute",
    "destroy:dev": "cdk destroy --profile dev",
    "diff:dev": "cdk diff --profile dev --no-fail",
    "diff:dev:json": "cdk diff --profile dev --no-fail --json",
    "diff:dev:stack": "cdk diff --profile dev --no-fail --stack",
    "diff:dev:resource": "cdk diff --profile dev --no-fail --resource",
    "list:dev": "cdk list --profile dev"
  },
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@types/node": "20.7.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "aws-cdk": "2.99.1",
    "ts-node": "^10.9.1",
    "typescript": "~5.2.2"
  },
  "dependencies": {
    "aws-cdk-lib": "2.189.1",
    "constructs": "^10.0.0",
    "source-map-support": "^0.5.21"
  }
} 
